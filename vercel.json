{
    "headers": [
        {
            "source": "/(.*)\\.(webp|jpg|jpeg|png|gif|svg|ico|css|js|woff|woff2|tff)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}